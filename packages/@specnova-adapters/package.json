{
  "type": "module",
  "name": "@rabel-lab/specnova-adapters",
  "private": false,
  "version": "0.0.1",
  "description": "OpenAPI SpecNova Adapters",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "format": "prettier --write 'src/**/*.ts'",
    "lint": "eslint --fix 'src/**/*.ts'"
  },
  "exports": {
    ".": {
      "import": "./dist/adapters/index.js",
      "require": "./dist/adapters/index.cjs"
    }
  },
  "devDependencies": {
    "@rabel-lab/specnova": "workspace:*",
    "@internal/specnova-eslint-config": "workspace:*",
    "@types/node": "^20.19.25",
    "tsup": "^8.5.1",
    "turbo": "^2.6.3",
    "typescript": "^5.9.3"
  },
  "optionalDependencies": {
    "@hey-api/openapi-ts": "^0.88.0"
  },
  "license": "MIT",
  "engines": {
    "node": ">=22"
  },
  "packageManager": "pnpm@10.19.0"
}
